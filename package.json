{
  "name": "budget-app-backend",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "node --env-file=dev.env --watch --trace-uncaught --trace-warnings --import tsx src/main.ts",
    "build": "tsc",
    "start": "node --env-file=development.env dist/index.js",
    "drizzle:generate": "NODE_ENV=dev drizzle-kit generate --config=./drizzle/drizzle.config.ts",
    "drizzle:generate:custom": "NODE_ENV=dev drizzle-kit generate --config=./drizzle/drizzle.config.ts --custom",
    "drizzle:migrate": "NODE_ENV=dev drizzle-kit migrate --config=./drizzle/drizzle.config.ts",
    "drizzle:seed:fake_users": "NODE_ENV=dev tsx drizzle/seed/fake_users.ts",
    "drizzle:prod:migrate": "NODE_ENV=prod drizzle-kit migrate --config=./drizzle/drizzle.config.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^25.1.0",
    "@types/pg": "^8.16.0",
    "dotenv": "^17.3.1",
    "drizzle-kit": "^0.31.9",
    "pino-pretty": "^13.1.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "drizzle-orm": "^0.45.1",
    "express": "^5.2.1",
    "pg": "^8.18.0",
    "pino": "^10.3.1",
    "pino-http": "^11.0.0",
    "serverless-http": "^4.0.0",
    "zod": "^4.3.6"
  }
}
